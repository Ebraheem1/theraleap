{
  "name": "theraleap",
  "version": "0.1.0",
  "description": "Online Assisted Ergotherapy",
  "main": "index.js",
  "author": "Georg Grab",
  "license": "MIT",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/talkdirty/theraleap.git"
  },
  "jest": {
    "moduleFileExtensions": ["js", "jsx", "ts", "tsx", "json", "vue"],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": ["**/?(*.)(spec|test).ts?(x)"]
  },
  "scripts": {
    "test": "karma start karma.config.js",
    "jest": "jest",
    "dev":
      "check-node-version --package && webpack-dev-server --content-base dist --config webpack.config.js",
    "build":
      "check-node-version --package && webpack -p --config webpack.config.js",
    "deploy": "npx gh-pages --dist dist/",
    "precommit": "pretty-quick --staged"
  },
  "engines": {
    "node": ">=8 <9"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/d3": "^5.0.0",
