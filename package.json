    "jest": "^22.4.3",
    "jsdom": "^11.10.0",
    "jsdom-global": "^3.0.2",
    "node-sass": "4.7.1",
    "postcss-autoreset": "^2.0.2",
    "postcss-loader": "^2.1.3",
    "prettier": "^1.12.1",
    "pretty-quick": "^1.4.1",
    "sass-loader": "^7.0.1",
    "sinon": "^5.0.7",
    "style-loader": "^0.20.3",
    "sw-precache-webpack-plugin": "^0.11.5",
    "ts-jest": "^22.4.6",
